# === KUNCI & RPC ===
PRIVATE_KEY=0x                       # isi private key ed25519 hex, wajib
HTTP_URL=https://fullnode.mainnet.sui.io:443

# === PERFORMA & SAFETY ===
FAST_POLL_MS=150                          # interval polling event (ms)
SELL_LOCK_MS=1500                         # lock setelah 1x sell (ms)
SAFE_BPS=500                              # slippage 300 bps = 3%
QUIET_429=true                            # jangan spam log saat 429/throttle

# === ANTI-REPLAY (penting cegah auto-jual pas baru start) ===
COLD_START_GRACE_MS=3000                  # abaikan tx yg timestamp < start+3s

# === DETEKSI DEX (nama MoveCall, cocokkan lowercase, OR) ===
STRICT_DEX_NAMES=true                    # true = hanya nama di list yg lolos
DEX_NAME_LIST=cetus,clmm,pool,integrate,flowx,aggregator,swap_exact_input,swap_exact_output,swap_tokens,aftermath,amm,stableswap,turbos,router,bluemove,dex,deepbook,db_router,smart_router,route,swap,kriya,bluefin,route_all

# === DEX PACKAGES WAJIB (agar 100% baca FlowX) ===
DEX_PACKAGES=0xba153169476e8c3114962261d1edc70de5ad9781b83cc617ecc8c1923191cae0

# === EXCLUDE (BLACKLIST) â€” fair-launch / blast.fun ===
EXCLUDE_PACKAGES=0x779829966a2e8642c310bed79e6ba603e5acd3c31b25d7d4511e2c9303d6e3ef

# === DETEKSI PEMBAYARAN BUY (supaya FlowX & aggregator terbaca) ===
DETECT_ANY_PAY=true                       # true = anggap BUY sah walau bayar non-SUI
# DETECT_PAY_COINS=0x2::sui::SUI,0x<USDC>::USDC,0x<USDT>::USDT  # pakai ini kalau mau whitelist spesifik, dan set DETECT_ANY_PAY=false

# === ROUTING / FALLBACK ===
ALLOW_ROUTE_FALLBACK=true                 # boleh fallback route (tetap hormati blacklist)

# === GLOBAL WALKER (multi-token) ===
MULTI_WALKER=true                         # true = walker global ON

# === AUTO-TP / QUOTE POLLER ===
TP_POLL_MS=1200                           # interval cek TP (ms)

